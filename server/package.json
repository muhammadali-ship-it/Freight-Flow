{
  "name": "freightflow-server",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx api/index.ts",
    "build": "esbuild api/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "vercel-build": "esbuild *.ts services/*.ts shared/*.ts integrations/*.ts api/*.ts webhooks/*.ts --platform=node --packages=external --format=esm --outdir=dist --out-extension:.js=.js",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "import:csv": "tsx scripts/import-csv-data.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/form-data": "^2.2.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcryptjs": "^3.0.2",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.4",
    "memorystore": "^1.6.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.13",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
